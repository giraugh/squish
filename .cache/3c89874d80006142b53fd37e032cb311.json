lumn":2},"source":"src/corpseEntity.js","original":{"line":7,"column":27}},{"name":"module","generated":{"line":85,"column":0},"source":"src/corpseEntity.js","original":{"line":53,"column":0}},{"name":"exports","generated":{"line":85,"column":7},"source":"src/corpseEntity.js","original":{"line":53,"column":7}},{"generated":{"line":85,"column":14},"source":"src/corpseEntity.js","original":{"line":53,"column":0}},{"name":"CorpseEntity","generated":{"line":85,"column":17},"source":"src/corpseEntity.js","original":{"line":53,"column":17}},{"generated":{"line":85,"column":29},"source":"src/corpseEntity.js","original":{"line":53,"column":0}}],"sources":{"src/corpseEntity.js":"const Entity = require('./entity')\nconst tc = require('tinycolor2')\nconst { lerp } = require('./util')\n\nconst interp = x => Math.max(0, (0.5 * x) + Math.pow(1.8 * x - 1, 3))\n\nclass CorpseEntity extends Entity {\n  constructor (...args) {\n    // Has the same constructor signature\n    super(...args)\n\n    // fade out\n    this.alpha = 1\n    this.time = 0\n    this.fadeTime = 20\n\n    // no physics\n    this.ethereal = true\n  }\n\n  update (entities) {\n    // Keep track of time and calculate alpha\n    this.time++\n    this.alpha = 1 - (this.time / this.fadeTime)\n\n    // die when timer up\n    if (this.time >= this.fadeTime) {\n      this.remove = true\n    }\n\n    // Call super\n    super.update(entities)\n  }\n\n  draw (ctx) {\n    // Manipulate colour with alpha\n    let a = interp(this.alpha)\n    let c = tc(this.colour)\n    c.setAlpha(a)\n    ctx.fillStyle = c.toRgbString()\n\n    // Calculate pos and size for squish effect\n    let w = this.w\n    let h = lerp(this.h, 0, 1 - a)\n    let x = this.x + this.w / 2\n    let y = this.y\n\n    // Draw us\n    ctx.fillRect(x - w / 2, y + (this.h - h), w, h)\n  }\n}\n\nmodule.exports = CorpseEntity\n"},"lineCount":null}},"hash":"8a12a58ca7f6d6502e017762a60bce8d","cacheData":{"env":{}}}